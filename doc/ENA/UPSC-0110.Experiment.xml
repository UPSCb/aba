  <STUDY_REF refname="UPSC-0110"/>
  <DESIGN>
   <DESIGN_DESCRIPTION>Standard Illumina SE rnaseq protocol.</DESIGN_DESCRIPTION>
   <SAMPLE_DESCRIPTOR refname="UPSC-0110-S0001"/>
   <LIBRARY_DESCRIPTOR>
    <LIBRARY_NAME>0w-abi1_1</LIBRARY_NAME>
    <LIBRARY_STRATEGY>RNA-Seq</LIBRARY_STRATEGY>
    <LIBRARY_SOURCE>TRANSCRIPTOMIC</LIBRARY_SOURCE>
    <LIBRARY_SELECTION>RANDOM</LIBRARY_SELECTION>
    <LIBRARY_LAYOUT>
     <SINGLE/>
    </LIBRARY_LAYOUT>
    <LIBRARY_CONSTRUCTION_PROTOCOL>Standard Illumina SE protocol</LIBRARY_CONSTRUCTION_PROTOCOL>
   </LIBRARY_DESCRIPTOR>
