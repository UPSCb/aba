      <READ_CLASS>Application Read</READ_CLASS>
      <READ_TYPE>Forward</READ_TYPE>
      <BASE_COORD>1</BASE_COORD>
     </READ_SPEC>
    </SPOT_DECODE_SPEC>
   </SPOT_DESCRIPTOR>
  </DESIGN>
  <PLATFORM>
   <ILLUMINA>
    <INSTRUMENT_MODEL>Illumina HiSeq 1000</INSTRUMENT_MODEL>
   </ILLUMINA>
  </PLATFORM>
 </EXPERIMENT>
